generator client {
  provider = "prisma-client-js"
}

datasource grupoMorizDB {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model silo {
  id              Int      @id @default(autoincrement())
  silo_name       String   @default("----")
  sensor_value    Float    @default(0)
  dt_register     DateTime @default(now())
}